    <link rel="stylesheet" href="~/clark-master/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/clark-master/css/owl.theme.default.min.css">
    <link rel="stylesheet" href="~/clark-master/css/magnific-popup.css">

    <link rel="stylesheet" href="~/clark-master/css/aos.css">

    <link rel="stylesheet" href="~/clark-master/css/ionicons.min.css">

