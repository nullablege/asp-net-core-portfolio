                        <script src="~/clark-master/js/jquery.waypoints.min.js"></script>
                        <script src="~/clark-master/js/jquery.stellar.min.js"></script>
                        <script src="~/clark-master/js/owl.carousel.min.js"></script>
                        <script src="~/clark-master/js/jquery.magnific-popup.min.js"></script>
                        <script src="~/clark-master/js/aos.js"></script>
                        <script src="~/clark-master/js/jquery.animateNumber.min.js"></script>
                        <script src="~/clark-master/js/scrollax.min.js"></script>

                        <script src="~/clark-master/js/main.js"></script>

